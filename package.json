        "entry-tool": "git+https://github.com/entrylabs/entry-tool.git#dist/20231026",
        "@entrylabs/legacy-video": "^1.0.0",
        "excel4node": "^1.7.0",
        "fluent-ffmpeg": "^2.1.2",
        "fs-extra": "^8.1.0",
